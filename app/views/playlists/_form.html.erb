<%= form_for(@playlist) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
      <%= f.text_field :name, class: "form-control input-lg", autofocus: true, placeholder: "Name"  %>
   <br/>
      <%= f.submit 'Create Playlist', class: "btn btn-lg btn-warning", autofocus: true %>
<% end %>